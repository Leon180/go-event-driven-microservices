{
    "swagger": "2.0",
    "info": {
        "description": "Simple service for accounts resources",
        "title": "Accounts Service API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:7001",
    "basePath": "/v1/accounts",
    "paths": {
        "/account/create": {
            "post": {
                "description": "Create a new account",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "accounts"
                ],
                "summary": "Create a new account",
                "parameters": [
                    {
                        "description": "Account",
                        "name": "account",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/featuresdtos.CreateAccountRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Account created successfully",
                        "schema": {
                            "$ref": "#/definitions/customizeginresponse.JSONResponse"
                        }
                    }
                }
            }
        },
        "/account/delete": {
            "post": {
                "description": "Delete an account",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "accounts"
                ],
                "summary": "Delete an account",
                "parameters": [
                    {
                        "description": "Account",
                        "name": "account",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/featuresdtos.DeleteAccountRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "account deleted successfully",
                        "schema": {
                            "$ref": "#/definitions/customizeginresponse.JSONResponse"
                        }
                    }
                }
            }
        },
        "/account/restore": {
            "post": {
                "description": "Restore an account",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "accounts"
                ],
                "summary": "Restore an account",
                "parameters": [
                    {
                        "description": "Account",
                        "name": "account",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/featuresdtos.RestoreAccountRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "account restored successfully",
                        "schema": {
                            "$ref": "#/definitions/customizeginresponse.JSONResponse"
                        }
                    }
                }
            }
        },
        "/account/update": {
            "put": {
                "description": "Update an account",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "accounts"
                ],
                "summary": "Update an account",
                "parameters": [
                    {
                        "description": "Account",
                        "name": "account",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/featuresdtos.UpdateAccountRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "account updated successfully",
                        "schema": {
                            "$ref": "#/definitions/customizeginresponse.JSONResponse"
                        }
                    }
                }
            }
        },
        "/accounts/get": {
            "post": {
                "description": "Get accounts by mobile number",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "accounts"
                ],
                "summary": "Get accounts by mobile number",
                "parameters": [
                    {
                        "description": "Account",
                        "name": "account",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/featuresdtos.GetAccountsByMobileNumberRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "accounts retrieved successfully",
                        "schema": {
                            "$ref": "#/definitions/customizeginresponse.JSONResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "customizeginresponse.APIError": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "message": {
                    "type": "string"
                },
                "status": {
                    "type": "integer"
                }
            }
        },
        "customizeginresponse.JSONResponse": {
            "type": "object",
            "properties": {
                "data": {},
                "error": {
                    "$ref": "#/definitions/customizeginresponse.APIError"
                },
                "message": {
                    "type": "string"
                },
                "success": {
                    "type": "boolean"
                }
            }
        },
        "featuresdtos.CreateAccountRequest": {
            "type": "object",
            "required": [
                "account_type",
                "branch",
                "mobile_number"
            ],
            "properties": {
                "account_type": {
                    "description": "@enum \"checking\", \"savings\", \"currency\", \"salary\", \"business\"",
                    "type": "string"
                },
                "branch": {
                    "description": "@enum \"台北市中山區\", \"台北市松山區\", \"台北市信義區\", \"台北市文山區\", \"台北市北投區\", \"台北市南港區\", \"台北市萬華區\"",
                    "type": "string"
                },
                "mobile_number": {
                    "description": "@example 0958308280",
                    "type": "string"
                }
            }
        },
        "featuresdtos.DeleteAccountRequest": {
            "type": "object",
            "required": [
                "id"
            ],
            "properties": {
                "id": {
                    "type": "string"
                }
            }
        },
        "featuresdtos.GetAccountsByMobileNumberRequest": {
            "type": "object",
            "required": [
                "mobile_number"
            ],
            "properties": {
                "mobile_number": {
                    "type": "string"
                }
            }
        },
        "featuresdtos.RestoreAccountRequest": {
            "type": "object",
            "required": [
                "id"
            ],
            "properties": {
                "id": {
                    "type": "string"
                }
            }
        },
        "featuresdtos.UpdateAccountRequest": {
            "type": "object",
            "required": [
                "id"
            ],
            "properties": {
                "account_number": {
                    "type": "string"
                },
                "branch_address": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "mobile_number": {
                    "type": "string"
                }
            }
        }
    }
}